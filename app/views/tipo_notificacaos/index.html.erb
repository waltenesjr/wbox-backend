<p id="notice"><%= notice %></p>

<h1>Listing Tipo Notificacaos</h1>

<table>
  <thead>
    <tr>
      <th>Qtd dias</th>
      <th>Mensagem</th>
      <th>Ativo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_notificacaos.each do |tipo_notificacao| %>
      <tr>
        <td><%= tipo_notificacao.qtd_dias %></td>
        <td><%= tipo_notificacao.mensagem %></td>
        <td><%= tipo_notificacao.ativo %></td>
        <td><%= link_to 'Show', tipo_notificacao %></td>
        <td><%= link_to 'Edit', edit_tipo_notificacao_path(tipo_notificacao) %></td>
        <td><%= link_to 'Destroy', tipo_notificacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo notificacao', new_tipo_notificacao_path %>
